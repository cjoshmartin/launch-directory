<h1>Twitter</h1>

<table>
    <tr>
        <th>name</th>
        <th> Bio</th>
        <th> Twitter handle</th>
    </tr>

    <% @entities.each do |entity| %>
    <tr>
      <td><%= entity.name %></td>
      <td><%= entity.description %></td>
      <td><%= entity.twitter %></td>
    </tr>
    <% end %>
</table>

    <br>
    <%= link_to "entity", new_entity_path %>
